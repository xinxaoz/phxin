m = 1071
if m>1:
    for i in range(2,m):
        if m%i==0:
            print (m,"is not prime number")
            print (i," / ",m//i," is ", m)
            break
        else:
            print (m,"is prime number")
    else:
        print (m," is not prime number")
d = 2
while m>1:
    if m%d==0:
        m=m//d
        print ("have ",d)
    else:
        d=d+1

